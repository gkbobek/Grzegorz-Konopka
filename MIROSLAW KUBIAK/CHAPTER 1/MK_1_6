//
/*Napisz program, który oblicza resztę z dzielenia całkowitego dla dwóch liczb całkowitych a = 37 i b = 11.*/
#include "stdafx.h"
#include <iostream>
using namespace std;


int main()
{
	int a, b;
	cout << "Give me a integer: ";
	cin >> a;
	cout << "Give me b integer: ";
	cin >> b;
	cin.get();
	cout << "Modulo a/b = " << a % b;
	cin.get();
    return 0;
}
