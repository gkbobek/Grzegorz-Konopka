/*Napisz program, który za pomocą instrukcji do ... while sumuje liczby nieparzyste w przedziale od 1 do 100.*/

#include "stdafx.h"
#include <iostream>
using namespace std;

int main()
{
	int sum = 0;
	int i = 1;
	do
	{
		if (i % 2 != 0)
			sum += i;
		i++;
	} while (i <= 100);
	cout << "Sum of odd numbers from 1 to 100: " << sum;
	cin.get();
	return 0;
}
