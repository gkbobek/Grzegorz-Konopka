/*Napisz program, który za pomocą instrukcji while wyświetla liczby całkowite od 1 do 20.*/

#include "stdafx.h"
#include <iostream>
using namespace std;

int main()
{
	int x = 1;
	cout << "This program will give you integer from 1 to 20" << endl;
	while (x <= 20)
	{
		cout << x << endl;
		x++;
	}
	cin.get();
  	return 0;
}
