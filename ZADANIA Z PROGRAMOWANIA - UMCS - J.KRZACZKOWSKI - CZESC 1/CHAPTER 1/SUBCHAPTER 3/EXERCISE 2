#include "stdafx.h"
#include <iostream>
using namespace std;

int main()
{
	int x;
	int y;
	cout <<  "Give me 2 numbers. Program will tell you which one is bigger\n";
	cout << "Give me first: ";
	cin >> x;
	cout << "Give me second: ";
	cin >> y;
	cin.get();
	if (x < y)
		cout << y;
	else if (x > y)
		cout << y;
	else
		cout << "They are equal!";
	cin.get();
    	return 0;
}

